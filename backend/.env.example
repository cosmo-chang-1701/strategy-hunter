# Environment Variables Configuration
# Copy this file to .env and fill in the actual values

# API key for connecting to Polygon.io (required for market data)
POLYGON_API_KEY=YOUR_POLYGON_API_KEY_HERE

# Secret key for JWT signing (use a complex random string)
SECRET_KEY=your_super_secret_key_for_jwt_here

# JWT signing algorithm (default: HS256)
ALGORITHM=HS256

# Access Token expiration time in minutes (default: 30)
ACCESS_TOKEN_EXPIRE_MINUTES=30